<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" minHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameMenuController">
   <children>
      <PointLight color="CHARTREUSE" lightOn="true" />
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/game_menu/graphics/GameMenu.png" />
         </image>
      </ImageView>
      <Button fx:id="newGameButton" layoutX="841.0" layoutY="472.0" mnemonicParsing="false" onAction="#loadNewGame" prefHeight="82.0" prefWidth="239.0" text="New Game" textFill="#6e0000" />
      <Button fx:id="creditsButton" layoutX="841.0" layoutY="642.0" mnemonicParsing="false" onAction="#loadCredits" prefHeight="82.0" prefWidth="239.0" text="Credits" textFill="#6e0000" />
      <Button fx:id="exitGameMenuButton" layoutX="841.0" layoutY="812.0" mnemonicParsing="false" onAction="#closeGameMenuAction" prefHeight="82.0" prefWidth="239.0" text="EXIT" textFill="#6e0000" />
   </children>
</AnchorPane>
