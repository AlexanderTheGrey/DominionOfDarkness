<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CityGravesController">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/in_game/graphics/City%20of%20Graves.jpg" />
         </image>
      </ImageView>
      <Button fx:id="cityGravesNexusButton" layoutX="43.0" layoutY="963.0" mnemonicParsing="false" onAction="#loadNexus" prefHeight="82.0" prefWidth="290.0" text="Back to the Nexus" />
      <Button fx:id="cityGravesExitButton" layoutX="1816.0" layoutY="14.0" mnemonicParsing="false" onAction="#closeGameAction" prefHeight="33.0" prefWidth="90.0" text="EXIT" />
   </children>
</AnchorPane>
